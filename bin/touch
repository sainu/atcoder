#!/usr/bin/env ruby
# frozen_string_literal: true

filepath = ARGV[0]
unless File.exist?(filepath)
  File.open(filepath, 'w') do |f|
    template = <<-EOT
# frozen_string_literal: true

require 'bundler/setup'
require 'pry'

# ----- ここから下が提出コード -----
def gi; gets.to_i end
def d(n); Math.log10(n).to_i + 1 end

    EOT
    f.puts template
  end
end
